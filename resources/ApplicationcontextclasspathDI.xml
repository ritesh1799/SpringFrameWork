<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <bean id="student" class="com.SpringProject.di.setter.Student">


            <property name = "studentId" value = "1"/>
            <property name = "studentName" value = "Ritesh"/>
            <property name = "mark" ref = "mark"/>


</bean>

    <bean id = "mark" class = "com.SpringProject.di.setter.Mark">
        <property name="mark1" value = "50"/>
        <property name="mark2" value= "60"/>
    </bean>
</beans>